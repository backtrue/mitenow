# mite.now Frontend Environment Variables
# Copy this file to frontend/.env.local and fill in the values

# ============================================
# API Configuration
# ============================================
# Backend API URL (leave empty for same-origin)
NEXT_PUBLIC_API_URL=

# ============================================
# Sentry (Optional)
# ============================================
# Sentry DSN for frontend error tracking
NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxx

# ============================================
# App Version
# ============================================
NEXT_PUBLIC_APP_VERSION=1.0.0
